#include <stdio.h>
#include <stdlib.h>

int main () {

    
// aloca matriz M * N
    int **M1; // p tem duas dimensoes

    // criar array de ponteiros
    M1 = (int **) malloc(3 * sizeof(int *)); // linhas matriz 

    for(int i = 0; i < 2; i++) {
        
        M1[i] = (int *) malloc(2 * sizeof(int)); // colunas
    }

// aloca matriz N * P

    int **M2;

    M2 = (int **) malloc(2 * sizeof(int **)); // linhas

    for(int j = 0; j < 3; j++){
        M2[j] = (int *) malloc(3 * sizeof(int)); // aloca colunas
    }

// aloca Matriz resultado 

    int **R;

    R = (int **) malloc(3 * sizeof(int **));

    for(int z = 0; z < 3; z++){
        R[z] = (int *) malloc (3 * sizeof(int));
    }


    return 0;
}
